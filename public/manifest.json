{
  "manifest_version": 3,
  "name": "WS by zimo",
  "version": "1.0",
  "description": "Custom Scrollbar",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content.css"]
    }
  ]
}
