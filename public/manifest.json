{
  "manifest_version": 3,
  "name": "WS by ZIMO",
  "version": "1.0",
  "description": "WS | ZIMO | Official Website | The Ultimate URL Shortening Service.",
  "permissions": ["tabs", "geolocation"],
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content.css"]
    }
  ]
}
